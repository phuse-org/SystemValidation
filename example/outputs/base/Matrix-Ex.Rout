object="indMatrix"
function (object, ...) 
{
    d <- dim(object)
    if (d[1L] != d[2L]) 
        FALSE
    else !any(object != t(object))
}


object="sparseMatrix"
function (object, ...) 
{
    .local <- function (object, tol = 100 * .Machine$double.eps, 
        ...) 
    {
        d <- dim(object)
        if (d[1] != d[2]) 
            return(FALSE)
        if (is(object, "dMatrix")) 
            isTRUE(all.equal(.as.dgC.0.factors(object), .as.dgC.0.factors(t(object)), 
                tolerance = tol, ...))
        else if (is(object, "lMatrix")) 
            identical(as(object, "lgCMatrix"), as(t(object), 
                "lgCMatrix"))
        else if (is(object, "nMatrix")) 
            identical(as(object, "ngCMatrix"), as(t(object), 
                "ngCMatrix"))
        else stop("not yet implemented")
    }
    .local(object, ...)
}


