Function: isSymmetric (package base)
object="ANY"
function (object, ...) 
UseMethod("isSymmetric")


object="denseMatrix"
function (object, ...) 
{
    .local <- function (object, tol = 100 * .Machine$double.eps, 
        tol1 = 8 * tol, ...) 
    {
        d <- dim(object)
        if ((n <- d[1L]) != d[2L]) 
            return(FALSE)
        if (n <= 1L) 
            return(TRUE)
        is.z <- is(object, "zMatrix")
        if (length(tol1)) {
            Cj <- if (is.z) 
                Conj
            else identity
            for (i in unique(c(1L, 2L, n - 1L, n))) if (is.character(all.equal(object[i, 
                ], Cj(object[, i]), tolerance = tol1, ...))) 
                return(FALSE)
        }
        if (is(object, "dMatrix")) 
            isTRUE(all.equal(as(object, "dgeMatrix"), as(t(object), 
                "dgeMatrix"), tolerance = tol, ...))
        else if (is(object, "nMatrix")) 
            identical(as(object, "ngeMatrix"), as(t(object), 
                "ngeMatrix"))
        else if (is(object, "lMatrix")) 
            identical(as(object, "lgeMatrix"), as(t(object), 
                "lgeMatrix"))
        else if (is.z) 
            isTRUE(all.equal(as(object, "zgeMatrix"), as(Conj(t(object)), 
                "zgeMatrix"), tolerance = tol, ...))
        else if (is(object, "iMatrix")) 
            identical(as(object, "igeMatrix"), as(t(object), 
                "igeMatrix"))
    }
    .local(object, ...)
}


object="dgCMatrix"
function (object, ...) 
{
    .local <- function (object, tol = 100 * .Machine$double.eps, 
        ...) 
    isTRUE(all.equal(.dgC.0.factors(object), t(object), tolerance = tol, 
        ...))
    .local(object, ...)
}


object="diagonalMatrix"
