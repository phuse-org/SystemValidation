> op <- options(keep.source = TRUE) # and so, these have differing "srcref" :
> f1 <- function() {}
> f2 <- function() {}
> identical(f1,f2)# ignore.srcref= TRUE : TRUE
[1] TRUE
> identical(f1,f2,  ignore.srcref=FALSE)# FALSE
[1] FALSE
> options(op) # revert to previous state
