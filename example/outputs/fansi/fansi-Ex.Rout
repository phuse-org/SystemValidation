+   c(
+     0, 1, 0,  # red output is green input
+     0, 0, 1,  # green output is blue input
+     1, 0, 0   # blue output is red input
+   ),
+   nrow=3, byrow=TRUE
+ )
> css2 <- make_styles(classes, rgb.mix=mix)
> ## Display in browser: same HTML but colors changed by CSS
