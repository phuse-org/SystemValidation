function (file, driver, syntax, encoding = "UTF-8", quiet = FALSE, 
    ...) 
{
    {
        on.exit({
            opts_chunk$restore()
            knit_hooks$restore()
        }, add = TRUE)
        oopts = options(markdown.HTML.header = NULL, knitr.knit2html.force_v1 = TRUE)
        on.exit(options(oopts), add = TRUE)
    }
    opts_chunk$set(error = FALSE)
    knit_hooks$set(purl = hook_purl)
    (if (grepl("\\.[Rr]md$", file)) 
        knit2html
    else if (grepl("\\.[Rr]rst$", file)) 
        knit2pandoc
    else knit)(file, encoding = encoding, quiet = quiet, envir = globalenv(), 
        ...)
}
